def is_balanced(expression): 
    stack = [] 
    pairs = {')': '(', ']': '[', '}': '{'} 
     
    for char in expression: 
        if char in '([{': 
            stack.append(char) 
        elif char in ')]}': 
            if not stack or stack.pop() != pairs[char]: 
                return False 
    return not stack 
 
# Input 
expr = input("Enter an expression: ") 
if is_balanced(expr): 
    print(â€œ Parentheses are balanced.") 
else: 
    print("Parentheses are NOT balanced.")
