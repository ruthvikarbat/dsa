def bucket_sort(arr):
    bucket = []
    slot_num = 10  

    for i in range(slot_num):
        bucket.append([])

    for j in arr:
        index_b = int(slot_num * j / 100)
        if index_b == slot_num:   
            index_b = slot_num - 1
        bucket[index_b].append(j)

    for i in range(slot_num):
        bucket[i].sort()

    sorted_list = []
    for i in range(slot_num):
        sorted_list.extend(bucket[i])

    return sorted_list

n = int(input("Enter number of students: "))
percentages = [float(input(f"Enter 12th percentage of student {i+1}: ")) for i in range(n)]


sorted_list = bucket_sort(percentages)


print("\nSorted Percentages (Ascending):", sorted_list)
print("\nTop 5 Percentages:", sorted_list[-5:][::-1])
